{
  "name": "mtonzar/shared-services-bundle",
  "description": "Bundle Symfony partagÃ© avec services communs (health checks, API Platform, etc.)",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["symfony", "bundle", "api-platform", "shared-services", "health-check"],
  "authors": [
    {
      "name": "mohamed tonzar",
      "email": "mtonzar@saphir-innovation.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "mtonzar\\SharedServicesBundle\\": "src/SharedServicesBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mtonzar\\SharedServicesBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "api-platform/core": "^3.4",
    "doctrine/dbal": "^3.0|^4.0",
    "doctrine/doctrine-bundle": "^2.16",
    "doctrine/orm": "^3.5",
    "symfony/cache": "^6.4|^7.0",
    "symfony/config": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/http-client": "^6.4|^7.0",
    "symfony/messenger": "^6.4|^7.0",
    "symfony/runtime": "^6.4|^7.0",
    "symfony/serializer": "^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "symfony/phpunit-bridge": "^6.4"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
